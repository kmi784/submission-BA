@article{Kim2021,
	abstract = {The square-lattice Ising model with nearest-neighbor ({\$}{\$}J{\_}1{\$}{\$}) and next-nearest-neighbor ({\$}{\$}J{\_}2{\$}{\$}) interactions is exactly unsolvable. The square-lattice {\$}{\$}J{\_}1-J{\_}2{\$}{\$}Ising model is frustrated for {\$}{\$}J{\_}2<0{\$}{\$}. For {\$}{\$}R=J{\_}2/J{\_}1={$\backslash$}pm 1/2{\$}{\$}, the square-lattice {\$}{\$}J{\_}1-J{\_}2{\$}{\$}Ising model for {\$}{\$}J{\_}2<0{\$}{\$}is the most frustrated, and its ground states are infinitely degenerate. The exact integer values for the density of states of the {\$}{\$}J{\_}1-J{\_}2{\$}{\$}Ising model for {\$}{\$}R={$\backslash$}pm 1/2{\$}{\$}are evaluated on {\$}{\$}L{$\backslash$}times 2L{\$}{\$}square lattices with free boundary conditions in the L-direction and periodic boundary conditions in the 2L-direction up to {\$}{\$}L=12{\$}{\$}using an exact enumeration method. The total number of states is {\$}{\$}2\^{}{\{}288{\}}{$\backslash$}approx 5{$\backslash$}times 10\^{}{\{}86{\}}{\$}{\$}for {\$}{\$}L=12{\$}{\$}, and counting all {\$}{\$}2\^{}{\{}288{\}}{\$}{\$}states requires enormous computational work. The thermal scaling exponent {\$}{\$}y{\_}t=1(=1/{$\backslash$}nu ){\$}{\$}(where {\$}{\$}{$\backslash$}nu {\$}{\$}is the correlation-length critical exponent) of the square-lattice {\$}{\$}J{\_}1-J{\_}2{\$}{\$}Ising model is obtained for {\$}{\$}J{\_}2>0{\$}{\$}and {\$}{\$}R={$\backslash$}pm 1/2{\$}{\$}, in agreement with the Ising universality class. The shift exponent {\$}{\$}{$\backslash$}lambda =1.00{\$}{\$}is obtained for {\$}{\$}J{\_}2>0{\$}{\$}and {\$}{\$}R={$\backslash$}pm 1/2{\$}{\$}, equaling the thermal scaling exponent {\$}{\$}y{\_}t{\$}{\$}. On the other hand, the thermal scaling exponent {\$}{\$}y{\_}t=2.0{\$}{\$}of the square-lattice {\$}{\$}J{\_}1-J{\_}2{\$}{\$}Ising model is obtained for {\$}{\$}J{\_}2<0{\$}{\$}and {\$}{\$}R={$\backslash$}pm 1/2{\$}{\$}, suggesting a first-order phase transition. The shift exponent {\$}{\$}{$\backslash$}lambda =1.1{\$}{\$}is obtained for {\$}{\$}J{\_}2<0{\$}{\$}and {\$}{\$}R={$\backslash$}pm 1/2{\$}{\$}and is different from the thermal scaling exponent {\$}{\$}y{\_}t{\$}{\$}.},
